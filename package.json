{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "mix && npm run clear-cache",
        "production": "mix --production && npm run clear-cache",
        "prod": "npm run production",
        "watch": "mix watch",
        "start": "git pull origin master && npm run prod",
        "start-dev": "git pull origin dev && npm run dev",
        "start-staging": "git pull origin staging && npm run prod",
        "clear-cache": "php wp-cli.phar wpacu_clear_assets_cache --allow-root"
    },
    "dependencies": {
