        "vue-loading-overlay": "^3.2.0",
        "vue-router": "^3.5.1",
        "vue-select": "^2.6.0",
        "vue-youtube": "^1.4.0",
        "vuex": "^3.1.0",
        "vuex-persistedstate": "^2.5.4"
    },
    "devDependencies": {
        "acorn": "^8.0.5",
        "babel-eslint": "^10.1.0",
        "cross-env": "^7.0.3",
        "eslint": "^7.20.0",
        "import-glob-loader": "^1.1.0",
        "laravel-mix": "^6.0.11",
        "postcss": "^8.2.6",
        "resolve-url-loader": "^3.1.2",
